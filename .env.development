# ===========================================
# Unum Development Environment
# ===========================================
# NOTE: This file contains NO secrets and is safe to commit to git.
# Cognito Identity Pool ID is just an identifier, not a credential.

# Environment
APP_ENV=development

# AWS Region
AWS_REGION=us-east-1

# Cognito Identity Pool (NOT a secret - just an identifier)
# Replace with your actual Identity Pool ID from Terraform output
COGNITO_IDENTITY_POOL_ID=us-east-1:97c8dfa9-8660-4879-9520-f0793d3e3c79

# Resource Names (from Terraform output)
DYNAMO_TABLE=unum-dev
S3_BUCKET=unum-media-dev-7x4k2

# Auth Backend API
AUTH_API_URL=https://qj9x18vc59.execute-api.us-east-1.amazonaws.com

# Feature Flags
USE_AWS_BACKEND=true
ENABLE_OFFLINE_SYNC=true
ENABLE_BACKGROUND_SYNC=false
USE_TEST_DATA=true
DEBUG=true
